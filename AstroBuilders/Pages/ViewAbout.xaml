<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="AstroBuilders.ViewAbout" 
xmlns:local="clr-namespace:AstroBuilders;assembly=AstroBuilders">
	<ContentView.Content>
	<StackLayout Orientation="Vertical" Padding="6,12,6,12">
		<local:StarJedi Text="A propos de" TextColor="{local:ColorTextResource}" Font="Medium" HorizontalOptions="Center" />
		<local:AurekBesh Text="A propos de" TextColor="{local:ColorTextResource}" Font="Micro" HorizontalOptions="Center" />
		<BoxView HeightRequest="12" BackgroundColor="Transparent" />
		<StackLayout HorizontalOptions="Fill" VerticalOptions="Center" BackgroundColor="{local:ColorBgResource MiniBorder}" Padding="3">
		<StackLayout HorizontalOptions="Fill" VerticalOptions="Center" BackgroundColor="{local:ColorBgResource}" Padding="6">
		<StackLayout HorizontalOptions="Fill" VerticalOptions="Center" BackgroundColor="{local:ColorBgResource Border}" Padding="6">
		<StackLayout HorizontalOptions="Fill" VerticalOptions="Center" BackgroundColor="{local:ColorBgResource Box}" Padding="4">
			<StackLayout x:Name="theStack" Orientation="Vertical"/>
		</StackLayout>
		</StackLayout>
		</StackLayout>
		</StackLayout>
		<BoxView HeightRequest="12" BackgroundColor="Transparent" />
		<WebView x:Name="theWebview" VerticalOptions="FillAndExpand" />
	</StackLayout>
	</ContentView.Content>
</ContentView>
